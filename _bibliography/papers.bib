@article{stewens_billigkeit_2022,
  title = {{Billigkeit, Sittlichkeit, Menschlichkeit: Versuch einer Kritik der Generalklauseln im V{\"o}lkerrecht}},
  shorttitle = {{Billigkeit, Sittlichkeit, Menschlichkeit}},
  author = {Stewens, Paul Philipp},
